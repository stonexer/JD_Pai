{
	"manifest_version" : 2,
	"name" : "京东夺宝小助手",
	"description" : "京东夺宝小助手",
	"version" : "0.2",
	"permissions" : [
		"tabs",
		"http://paimai.jd.com/*"
	],
	"icons" : {
		"16":"imgs/S16.png", 
		"48":"imgs/S48.png", 
		"128":"imgs/S128.png"
	},
	"web_accessible_resources" : [
		"js/jquery.min.js", "js/jd.js"
	],
	"content_scripts" : [
		{
			"matches" : ["http://paimai.jd.com/*"],
			"css" : ["css/jd.css"],
			"js" : ["js/jquery.min.js", "js/jd.js"]
		}
	],
	"browser_action" : {
		"default_icon" : "imgs/S128.png",
		"default_title" : "京东夺宝岛小助手",
		"default_popup": "popup.html"
	}
}